        default_value="[% pref.default_value %]">
    [%- IF pref.datatype == 'enum' -%]
        [%- FOR opt = pref.options %]
        <EnumValue value="[% opt.0 %]" display_value="[% opt.1 %]"/>
        [%- END %]
    [%- END %]
