<div class="lightbox" id="st-self-join-workspaceinterface">

  <div class="title">[% loc('do.join-to-edit') %]</div><br />
  <div class="content"></div>
  <div class="error" style="display:none;"></div>

  <div class="buttons" id="st-self-join-workspace-buttons">
      <div class="loading" style="display:none;">
        <img src="/static/skin/common/images/ajax-loader.gif"/>
      </div>
      <ul class="widgetButton" style="float:left; padding:10px">
          <li class="flexButton" id="st-self-join-workspace-action">
              <a class="genericOrangeButton" href="#">[% loc('do.join') %]</a>
          </li>
          <li class="flexButton" id="st-self-join-workspace-closebutton">
              <a class="genericOrangeButton" href="#">[% loc('do.cancel') %]</a>
          </li>
      </ul>
  </div>

</div>
