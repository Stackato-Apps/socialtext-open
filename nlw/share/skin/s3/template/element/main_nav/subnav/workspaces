<li class="submenu"><a id="st-wiki-subnav-link-workspaces" href="/?action=workspaces_listall">[% loc("nav.wikis") %]</a>
  <ul class="level2" id="global-workspace-list">
  [% IF user.is_guest %]
    <li class="emptynotice">[% loc("info.wikis") %]</li>
  [% END %]
  </ul>

  [% UNLESS user.is_guest %]
    <script>
        $("#global-workspace-list").navList([
            {
                title: loc("wiki.all"),
                href: "/?action=workspaces_listall"
            },
            {
                data: Socialtext.workspaces,
                defaultId: 'default-workspace-link',
                href: function(w) { return '/' + w.name },
                title: function(w) { return w.label },
                sort: function(a,b) {
                    if (a['default']) return -1;
                    if (b['default']) return 1;
                    if (a.name == b.title) return 0;
                    return (a.title < b.title) ? -1 : 1;
                },
                emptyMessage:
                    loc("nav.no-wikis")
            }
        ]);
    </script>
  [% END %]
</li>
