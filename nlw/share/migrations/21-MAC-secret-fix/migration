#!/usr/bin/env perl
# @COPYRIGHT@

use strict;
use warnings;
use Socialtext::AppConfig;

my @words = <DATA>;
chomp @words;

my @secrets;
for ( 1..10 ) {
    push @secrets, $words[ rand @words ] . int( rand( 1000 ) );
}

my $config = Socialtext::AppConfig->new;
$config->set( MAC_secret =>  join( ' ', @secrets ) );
$config->write( file => $config->{file} );

# And now, 1,000 random words for building MAC secrets
